{
    "name": "av/bonefish",
    "description": "Bonefish Framework",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0",

        "av/eventdispatcher": "^1.2",
        "av/raptor": "^2.0",
        "av/bonefish-reflections": "^1.0",
        "av/bonefish-router": "^2.0",
        "av/bonefish-routecollectors": "^1.0",
        "av/bonefish-injection": "^1.0",
        "av/doctrine-annotations": "^1.0",

        "composer/installers": "^1.0",
        "tracy/tracy": "^2.3",
        "nette/neon": "^2.3"
    },
    "require-dev": {
       "phpunit/phpunit": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Bonefish\\": ["src"],
            "Bonefish\\Tests\\": ["tests"]
        }
    }
}