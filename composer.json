{
    "name": "av/bonefish",
    "description": "Bonefish Framework",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/AValnar/installers"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "composer/installers": "dev-custom-type-2",
        "av/bonefish-autoloader": "dev-master",
        "av/bonefish-di2": "dev-master",
        "nette/neon": "dev-master",
        "latte/latte": "dev-master",
        "tracy/tracy": "dev-master",
        "nette/reflection": "dev-master",
        "joetannenbaum/climate": "0.1.*",
        "league/url": "3.0.*@dev",
        "uestla/yetorm": "dev-master",
        "nikic/fast-route": "dev-master",
        "av/bonefish-helloworld": "dev-master",
        "av/bonefish-kickstart": "dev-master",
        "av/bonefish-generator": "dev-master",
        "av/bonefish-auth": "dev-master"
    },
    "extra": {
        "installer-paths": {
            "Packages/{$vendor}/{$name}/": ["type:bonefish-package"]
        }
    },
    "autoload": {
        "psr-4": {
            "Bonefish\\": ["src"],
            "Bonefish\\Tests\\": ["tests"]
        }
    }
}