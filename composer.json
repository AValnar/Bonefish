{
    "name": "av/bonefish",
    "description": "Bonefish Framework",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "composer/installers": "dev-master",
        "av/bonefish-autoloader": "1.*",
        "av/bonefish-di2": "2.*",
        "nette/neon": "2.3.*",
        "latte/latte": "2.3.*",
        "tracy/tracy": "2.3.*",
        "nette/reflection": "2.3.*",
        "league/url": "3.1.*",
        "nikic/fast-route": "0.4.*",
        "doctrine/common": "2.5.*"
    },
    "require-dev": {
       "phpunit/phpunit": "4.5.*"
    },
    "autoload": {
        "psr-4": {
            "Bonefish\\": ["src"],
            "Bonefish\\Tests\\": ["tests"]
        }
    }
}