{
    "name": "av/bonefish",
    "description": "Bonefish Framework",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/AValnar/installers"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "composer/installers": "dev-master",
        "av/bonefish-autoloader": "1.*",
        "av/bonefish-di2": "2.*",
        "nette/neon": "dev-master",
        "latte/latte": "dev-master",
        "tracy/tracy": "dev-master",
        "nette/reflection": "dev-master",
        "league/climate": "2.4.*",
        "league/url": "3.1.*",
        "uestla/yetorm": "dev-master",
        "nikic/fast-route": "0.2.*",
        "av/bonefish-helloworld": "1.*",
        "av/bonefish-kickstart": "1.*",
        "av/bonefish-generator": "1.*"
    },
    "require-dev": {
       "composer/composer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Bonefish\\": ["src"],
            "Bonefish\\Tests\\": ["tests"]
        }
    }
}