{
    "name": "av/bonefish",
    "description": "Bonefish Framework",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.0",
        "composer/installers": "dev-master",
        "av/bonefish-autoloader": "1.*",
        "tracy/tracy": "2.3.*",
        "av/bonefish-reflections": "dev-master",
        "av/bonefish-injection": "dev-master",
        "av/bonefish-utility": "dev-master",
        "av/bonefish-traits": "dev-master",
        "av/bonefish-router": "dev-master"
    },
    "require-dev": {
       "phpunit/phpunit": "4.5.*"
    },
    "autoload": {
        "psr-4": {
            "Bonefish\\": ["src"],
            "Bonefish\\Tests\\": ["tests"]
        }
    }
}